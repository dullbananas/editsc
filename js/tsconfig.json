{
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "AMD",
    "moduleResolution": "node",
    "target": "ES2016",
    "allowJs": true,
    "checkJs": false,
    "outFile": "../static/build/main.js",
    "incremental": true,
    "listFiles": false,
    "listEmittedFiles": true,
    "strictNullChecks": true,
    "allowUmdGlobalAccess": true,
    "types": ["jszip", "downloadjs"],
  },
}

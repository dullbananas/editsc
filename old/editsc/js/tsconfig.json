{
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES2017",
    "allowJs": true,
    "checkJs": false,
    "outDir": "../static/",
    "incremental": true,
    "listFiles": true,
    "listEmittedFiles": true,
    "strictNullChecks": true,
  },
  "exclude": [
    "node_modules",
    "src/old",
  ],
}
